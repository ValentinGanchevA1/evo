{
  "name": "evo",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "setup": "npm install && cd ios && pod install",
    "dev": "react-native start --reset-cache",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test:watch": "jest --watchAll",
    "build:android-debug": "react-native run-android --variant=debug",
    "build:ios-debug": "react-native run-ios --configuration Debug",
    "storybook-generate": "sb-rn-get-stories"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "prettier": {
    "endOfLine": "auto"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native/new-app-screen": "0.80.2",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.24",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/crypto-js": "^4.2.2",
    "axios": "^1.12.0",
    "crypto-js": "^4.2.0",
    "react": "19.1.0",
    "react-native": "0.80.2",
    "react-native-image-picker": "^8.2.1",
    "react-native-maps": "^1.25.0",
    "react-native-permissions": "^5.4.2",
    "react-native-vector-icons": "^10.3.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@gorhom/bottom-sheet": "5.1.8",
    "@react-native-community/cli": "19.1.2",
    "@react-native-community/cli-platform-android": "19.1.1",
    "@react-native-community/cli-platform-ios": "19.1.1",
    "@react-native-community/datetimepicker": "8.4.3",
    "@react-native-community/slider": "4.5.7",
    "@react-native/babel-preset": "0.80.2",
    "@react-native/eslint-config": "0.80.2",
    "@react-native/metro-config": "0.80.2",
    "@react-native/typescript-config": "^0.80.2",
    "@storybook/addon-ondevice-actions": "9.1.0",
    "@storybook/addon-ondevice-controls": "9.1.0",
    "@storybook/react-native": "9.1.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.1.0",
    "babel-loader": "8.4.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.1.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "^1.0.0",
    "react-dom": "19.1.0",
    "react-native-gesture-handler": "2.27.2",
    "react-native-reanimated": "4.0.1",
    "react-native-safe-area-context": "5.5.2",
    "react-native-svg": "15.12.1",
    "react-test-renderer": "19.1.0",
